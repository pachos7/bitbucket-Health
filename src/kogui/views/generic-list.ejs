<% include ./head %>
<body>
<% include ./header %>
<main role="main" class="col-sm-9 ml-sm-auto col-md-10 pt-3">
    <% if (genericList == null) { %>
       <tr><td>No List to display</td></tr>
    <% } else {  %>
        <p></p>
        <% if (title !== null) { %><h3><%- title %></h3><% }  %>
        <div class="card">
          <div class="card-body">
             <div class="table-responsive">
              <table class="table table-bordered table-sm" id="dataTable" width="100%" cellspacing="0">
                <thead class="thead-light"><th><% if (title !== null) { %><%= title %><% }  %></th></thead>
                <tbody>
              <% genericList.forEach(function(listItem) { %>
                   <tr><td><%- listItem %></td></tr>
            	<% });%>
              </tbody>
              </table>            
            </div>
            <% if (typeof additionalHTML !== 'undefined') { %>
              <%- additionalHTML %>
            <% } %>
          </div>
        </div>
    <% } %>
  </main>
<% include ./footer %>
</body>
</html>